Solution

class Solution {
    public int lengthOfLastWord(String s) {
        int l=0;
        String temp = s.trim();
        for(int i=0;i<temp.length();i++){
            if(temp.charAt(i)==' '){
                l=0;
            }
            else{
                l++;
            }
        }
        return l;
    }
}
